
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * int sayac = 0; for(int i=2; i<1000; i++) { for(int j=1; j<=i; j++) { if(i % j
		 * == 0 ) { sayac++; } } if(sayac == 2 ) { System.out.println(i); } sayac = 0; }
		 */
		
		int sum =0;
		
		for(int i = 1;i <10; i++) {
			for(int j=1; j<i; i++) {
				if(i % j== 0) {
					sum += i;
				}
				if(sum == i) {
					System.out.println("bu sayý mük sayýdýr." + i);
				}
			}
		}
	}

}
