
public class Main {

	public static void main(String[] args) {
		
		//1-1000 arasýndaki asal sayýlarý yazdýrma 
		
		  int sayac = 0; 
		  for(int i=2; i<1000; i++) { 
			  for(int j=1; j<=i; j++) { 
				  if(i % j == 0 ) { 
					  sayac++; 
					  } 
				  } 
			  if(sayac == 2 ) { 
				  System.out.println(i + " asal sayýdýr"); 
				  } 
			  sayac = 0; 
			}
		 
		//1-1000 arasýndaki mükemmel sayýlarý yazdýrma 
		  
		int sum =0;
		
		for(int i = 1;i <1000; i++) {
			for(int j=1; j<i; j++) {
				if(i % j== 0) {
					sum += j;
				}	
			}
			if(sum == i) {
				System.out.println("bu sayý mük sayýdýr " + i);
			}
			sum=0;
		}
	}

}
