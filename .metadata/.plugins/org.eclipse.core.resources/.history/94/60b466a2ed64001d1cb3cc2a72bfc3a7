package kodlamaIO;

public class Course {
	private int id;
	private String name;
	private String instructorName;
	private String image;
	private String completedState;
	
	private Category category;
	
	
	public Course() {
		category=new Category();
	}

	public Course(int id, String name, String instructorName, String image, String completedState, Category category) {
		
		this();
		this.id = id;
		this.name = name;
		this.instructorName = instructorName;
		this.image = image;
		this.completedState = completedState;
		this.category=category;
	}

	
	public Category getCategory() {
		return category;
	}

	public void setCategory(Category category) {
		this.category = category;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getInstructorName() {
		return instructorName;
	}

	public void setInstructorName(String instructorName) {
		this.instructorName = instructorName;
	}

	public String getImage() {
		return image;
	}

	public void setImage(String image) {
		this.image = image;
	}

	public String getCompletedState() {
		return completedState;
	}

	public void setCompletedState(String completedState) {
		this.completedState = completedState;
	}
	
}
